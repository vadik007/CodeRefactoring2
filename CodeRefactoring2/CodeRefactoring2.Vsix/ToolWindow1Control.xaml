<UserControl x:Class="CodeRefactoring2.Vsix.ToolWindow1Control"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vsix="clr-namespace:CodeRefactoring2.Vsix"
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="MyToolWindow">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <Button Content="Load log" Click="button1_Click" Name="button1"/>
                <Button Content="thing2" Click="button1_Click" Name="button2"/>
            </StackPanel>
            <!--x:Static vsix:ToolWindow1Control-->
            <ListBox ItemsSource="{Binding Path=Items}" />
        </StackPanel>
    </Grid>
</UserControl>
